public class App {
    public static void main(String[] args) {
        
        char[][] matriz = {
            {'g','i','u','l'},
            {'i','a'},
            {'l','o','u','z'},
            {'a','d','a'}
        };

        
        for (int i = 0; i < matriz.length; i++) {
            for (int j = 0; j < matriz[i].length; j++) {
                System.out.print(matriz[i][j]);
            }
            System.out.println(); 
        }
    }
}

